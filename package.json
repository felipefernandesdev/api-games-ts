{
  "name": "api-games-ts",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:felipefernandesdev/api-games-ts.git",
  "author": "Felipe Sousa Developer <fesousalocutor@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint --fix"
    ]
  },
  "scripts": {
    "dev": "tsnd --exit-child --respawn --transpile-only --ignore-watch node_modules ./src/server.ts",
    "build": "tsc",
    "build:dev": "tsc -w",
    "start": "rm -rf ./dist && npm run build && node ./dist/server.js",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
